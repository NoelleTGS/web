{
    "name": "wysi",
    "module": "index.tsx",
    "type": "module",
    "devDependencies": {
        "@types/bun": "latest",
        "daisyui": "latest",
        "typed-html": "^3.0.1"
    },
    "scripts": {
        "dev": "bun run --watch index.ts",
        "css": "tailwindcss -i ./public/css/main_in.css -o ./public/css/main_out.css --watch",
        "prod": "bun run cssprod && bun run build && bun run index.ts",
        "cssprod": "tailwindcss -i ./public/css/main_in.css -o ./public/css/main_out.css --minify",
        "build": "bun run build.ts"
    },
    "peerDependencies": {
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@elysiajs/cookie": "^0.8.0",
        "@elysiajs/html": "^0.8.0",
        "@elysiajs/jwt": "^0.8.0",
        "@elysiajs/static": "^0.8.1",
        "elysia": "^0.8.17",
        "elysia-ip": "^1.0.7",
        "htmx": "^0.0.2",
        "marked": "^13.0.3",
        "moment": "^2.30.1",
        "mongoose": "^8.6.3",
        "osu-api-extended": "^2.8.52",
        "osu-db-parser": "^1.0.37",
        "tailwindcss": "^3.4.13"
    }
}
