{
    "name": "wysi",
    "module": "index.tsx",
    "type": "module",
    "devDependencies": {
        "@types/bun": "latest",
        "daisyui": "latest"
    },
    "scripts": {
        "dev": "bun run --watch src/index.ts",
        "css": "tailwindcss -i ./public/css/main_in.css -o ./public/css/main_out.css --watch",
        "prod": "bun run cssprod && bun run src/index.ts",
        "cssprod": "tailwindcss -i ./public/css/main_in.css -o ./public/css/main_out.css --minify",
        "build": "bun run build.ts"
    },
    "peerDependencies": {
        "typescript": "^5.3.3"
    },
    "dependencies": {
        "@2toad/profanity": "^3.0.0",
        "@elysiajs/cookie": "^0.8.0",
        "@elysiajs/html": "^0.8.0",
        "@elysiajs/jwt": "^0.8.0",
        "@elysiajs/static": "^0.8.1",
        "@kitajs/html": "^4.2.2",
        "bun-types": "^1.1.30",
        "elysia": "^0.8.17",
        "elysia-ip": "^1.0.7",
        "moment": "^2.30.1",
        "mongoose": "^8.7.1",
        "osu-db-parser": "^1.0.37",
        "tailwindcss": "^3.4.13"
    }
}
