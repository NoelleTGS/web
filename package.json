{
  "name": "wysi",
  "module": "index.tsx",
  "type": "module",
  "devDependencies": {
    "@types/bun": "latest",
    "daisyui": "latest",
    "typed-html": "^3.0.1"
  },
  "scripts": {
    "dev": "bun run --watch index.ts",
    "prod": "bun run index.ts",
    "css": "tailwindcss -i ./public/css/main_in.css -o ./public/css/main_out.css --watch",
    "cssprod": "tailwindcss -o ./public/css/main_out.css --minify",
    "compile": "bun build --compile --minify --sourcemap index.ts --outfile wysi"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "dependencies": {
    "@elysiajs/cookie": "^0.8.0",
    "@elysiajs/html": "^0.8.0",
    "@elysiajs/jwt": "^0.8.0",
    "@elysiajs/static": "^0.8.1",
    "elysia": "^0.8.15",
    "htmx": "^0.0.2",
    "moment": "^2.30.1",
    "mongoose": "^8.1.1",
    "osu-api-extended": "^2.8.1",
    "osu-db-parser": "^1.0.37",
    "tailwindcss": "^3.4.1"
  }
}
